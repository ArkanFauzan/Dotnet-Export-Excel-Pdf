@model IEnumerable<StudentExportApp.Models.Student>
@{
    ViewData["Title"] = "Daftar Mahasiswa";
}

<h2>@ViewData["Title"]</h2>
<div class="mb-3">
  <a class="btn btn-success" href="/download-excel">Download Excel</a>
  <a class="btn btn-danger" href="/download-pdf">Download PDF</a>
</div>
<div class="table-responsive">
  <table class="table table-striped table-hover align-middle">
    <thead class="table-dark">
      <tr><th>NIM</th><th>Nama</th><th>Email</th><th>Fakultas</th><th>Jurusan</th></tr>
    </thead>
    <tbody>
      @foreach(var s in Model)
      {
        <tr>
          <td>@s.Nim</td><td>@s.Nama</td><td>@s.Email</td><td>@s.Fakultas</td><td>@s.Jurusan</td>
        </tr>
      }
    </tbody>
  </table>
</div>